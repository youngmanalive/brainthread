{
  "name": "brainthread",
  "version": "1.0.0",
  "description": "Share your threads",
  "scripts": {
    "client-install": "npm install --prefix client",
    "client-build-dev": "npm run build-dev --prefix client",
    "client-build-prod": "npm run build-prod --prefix client",
    "client": "npm start --prefix client",
    "server-install": "npm install --prefix server",
    "server": "node server/app",
    "dev-server": "nodemon server/app",
    "dev": "concurrently \"npm run dev-server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youngmanalive/brainthread.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/youngmanalive/brainthread/issues"
  },
  "homepage": "https://github.com/youngmanalive/brainthread#readme",
  "dependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.6"
  }
}
